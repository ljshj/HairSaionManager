#import <Foundation/Foundation.h>

@interface KPUserInfo : NSObject {

    NSInteger   userID;
    NSString    *userName;
    NSInteger   maxFileSize;
    NSUInteger  quotaTotal;
    NSUInteger  quotaUsed;
}

@property(nonatomic, assign) NSInteger   userID;
@property(nonatomic, retain) NSString    *userName;
@property(nonatomic, assign) NSInteger   maxFileSize;
@property(nonatomic, assign) NSUInteger  quotaTotal;
@property(nonatomic, assign) NSUInteger  quotaUsed;

@end
